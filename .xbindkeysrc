"xbacklight -dec 10"
    XF86MonBrightnessDown
"xbacklight -inc 10"
    XF86MonBrightnessUp
"kbdlight down"
    XF86KbdBrightnessDown
"kbdlight up"
    XF86KbdBrightnessUp
"/home/phil/bin/volume.sh toggle"
    XF86AudioMute
"/home/phil/bin/volume.sh down"
    XF86AudioLowerVolume
"/home/phil/bin/volume.sh up"
    XF86AudioRaiseVolume
"urxvt -e 'ncmpcpp'"
    XF86LaunchA
"urxvt -e 'mutt'"
    XF86LaunchB
"ncmpcpp next"
    XF86AudioNext
"ncmpcpp prev"
    XF86AudioPrev
"ncmpcpp stop"
    XF86AudioStop
"ncmpcpp toggle && /home/phil/bin/mpc.sh ask"
    XF86AudioPlay
"xvkbd -xsendevent -text '\[Delete]'"
    Alt + BackSpace
"xvkbd -xsendevent -text '\[End]'"
    Alt + Right
"xvkbd -xsendevent -text '\[Home]'"
    Alt + Left
"xvkbd -xsendevent -text '\[Page_Down]'"
    Alt + Down
"xvkbd -xsendevent -text '\[Page_Up]'"
    Alt + Up
